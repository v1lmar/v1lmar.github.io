{
  "name": "Vilmar Almeida",
  "version": "1.0",
  "dependencies": {
    "normalize-css": "latest",
    "typicons": "latest",
    "jquery": "latest",
    "requirejs": "latest",
    "handlebars": "latest",
    "SimpleSlider": "latest",
    "Magnific-Popup": "latest",
    "velocity": "latest"
  },
  "install": {
    "path": {
      "css": "src/css",
      "js": "src/lib",
      "eot": "src/fonts",
      "svg": "src/fonts",
      "ttf": "src/fonts",
      "woff": "src/fonts",
      "woff2": "src/fonts",
      "otf": "src/fonts"
    },
    "sources": {
      "normalize-css": "bower_components/normalize-css/normalize.css",
      "typicons": [
        "bower_components/typicons/src/font/typicons.min.css",
        "bower_components/typicons/src/font/typicons.eot",
        "bower_components/typicons/src/font/typicons.svg",
        "bower_components/typicons/src/font/typicons.ttf",
        "bower_components/typicons/src/font/typicons.woff"
      ],
      "jquery": "bower_components/jquery/dist/jquery.min.js",
      "requirejs": "bower_components/requirejs/require.js",
      "handlebars": "bower_components/handlebars/handlebars.min.js",
      "SimpleSlider": "bower_components/simple-slider/dist/simpleslider.min.js",
      "Magnific-Popup": [
        "bower_components/Magnific-Popup/dist/jquery.magnific-popup.min.js",
        "bower_components/Magnific-Popup/dist/magnific-popup.css"
      ],
      "velocity": [
        "bower_components/velocity/velocity.min.js",
        "bower_components/velocity/velocity.ui.min.js"
      ]
    }
  }
}
